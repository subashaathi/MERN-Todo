{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {}
      }
    ],
    "routes": [
      {
        "src": "/api/todos",
        "dest": "http://localhost:3000/todos"
      },
      {
        "src": "/api/todos/:id",
        "dest": "http://localhost:3000/todos/:id"
      }
    ]
  }